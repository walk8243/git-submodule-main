{
  "name": "git-submodule-main",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "yarn run tsc",
    "start": "node index",
    "test": "yarn run nyc mocha **/*.test.ts"
  },
  "repository": "git@github.com:walk8243/git-submodule-main.git",
  "author": "walk8243 <walk8243@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts",
      "**/*.js"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true
  }
}
